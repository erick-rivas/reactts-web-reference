{
  "name": "react-ts-reference",
  "version": "0.1.0",
  "description": "Reference for the development of React.ts app",
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@types/react-router-dom": "4.3.1",
    "npm-run-all": "4.1.5",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ui/styles -o src/ui/styles/css",
    "watch-css": "npm run build-css & node-sass-chokidar src/ui/styles -o src/ui/styles/css --watch --recursive",
    "start-ts": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-ts",
    "build-ts": "react-scripts-ts build",
    "build": "npm-run-all build-css build-ts",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.11.0",
    "@types/react": "16.8.7",
    "@types/react-dom": "16.8.2",
    "typescript": "3.3.3333",
    "node-sass-chokidar": "1.3.4",
    "react-scripts-ts": "3.1.0"
  }
}
